{
  "globals": {
    "describe": true,
    "it": true,
    "phantom": true,
    "expect": true,
    "eventAgent": true
  },
  "parser": "babel-eslint",
  "rules": {
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "always"
    ],
    "no-console": 0,
    "eqeqeq": [
      2,
      "allow-null"
    ]
  },
  "env": {
    "browser": true,
    "node": true
  },
  "extends": "eslint:recommended"
}